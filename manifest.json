{
    "name": "PostLogger",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Store POST info",
    "permissions":[
        "*://*/*/",
        "webRequest",
        "storage"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "icons":{
        "16" : "images/icon16.png",
        "48" : "images/icon48.png",
        "128" : "images/icon128.png"    
    },
    "browser_action": {
        "default_title":"通信履歴",
        "default_popup": "html/popup.html",
        "default_icon": {
            "19" : "images/icon19.png",
            "38" : "images/icon38.png"
        }
    }
  }