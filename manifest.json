{
    "name": "PostLogger",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Store POST info",
    "permissions":[
        "*://*/*/",
        "webRequest",
        "storage"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_title":"通信履歴",
        "default_popup": "html/popup.html",
        "default_icon": {
            "32": "images/icon32.png"
        }
    }
  }